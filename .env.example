# Django Backend API URL
EXPO_PUBLIC_API_BASE_URL=http://127.0.0.1:8000/api/v1

# For production, use your production URL:
# EXPO_PUBLIC_API_BASE_URL=https://api.wiwebb.com/api/v1

# Mock Data Mode (Development Only)
# Set to 'true' to use local JSON mock data instead of hitting the backend API
# This is useful for frontend development when the backend is not available
# Note: Authentication endpoints will still require the real backend
EXPO_PUBLIC_USE_MOCK_DATA=true


